.section ".text.stub"

.p2align 5
.global main
main:
	pushq %rdi
	// call _lfi_thread_create(&_lfi_pause, &.Lret)
	leaq _lfi_pause(%rip), %rdi
	leaq .Lret(%rip), %rsi
	pushq %rsi
	jmp _lfi_thread_create
.p2align 5
.Lret:
	popq %rdi
	// load address of trampoline table and then exit
	leaq trampotable(%rip), %rdi
.p2align 5
.global _lfi_pause
_lfi_pause:
	mov $60, %rax
	syscall
	int3
.p2align 5
.global _lfi_retfn
_lfi_retfn:
	// special runtime call for invoking lfi_ret
	//lfi:disable
	.bundle_lock
	leaq 1f(%rip), %r11
	jmpq *24(%r14)
1:
	.bundle_unlock
	//lfi:enable
	int3

.section ".data.trampotable"
.global trampotable
trampotable:
{{- range $sym := .exported }}
	.quad {{$sym}}
{{- end }}
{{- range $sym := .exposed}}
	.quad {{$sym}}
{{- end}}
.section .note.GNU-stack,"",@progbits
